public class q2 {

// Find the minimum of the array:
    public static void main(String[] args) {
        int []a={5,4,7,6,7,0};// array taken from user
        int min=a[0];// we take a[0] to compare with others// for maximum
        for(int i = 1;i< a.length;i++)//for loop for checking of each element
        {
                if(min > a[i]){//comparing max with each element
                min= a[i];//if min is larger than element than we swap that min with a[i]
            }
               
        }            System.out.println("min is "+min);//printing the max




    
    }
}
